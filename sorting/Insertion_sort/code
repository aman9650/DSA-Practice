import java.util.Arrays;
public class Main
{
	public static void main(String[] args) {
	    int a[]={5,4,3,2,1};
	   int n=a.length;
	   int i=1;
	   while(i<n){
	       int j=i;
	       while(j>0 && a[j-1]>a[j]){
	           int temp=a[j-1];
	           a[j-1]=a[j];
	           a[j]=temp;
               j=j-1;
	       }
	       i=i+1;
	   }
	    System.out.println(Arrays.toString(a));
	    
	}
	
}
