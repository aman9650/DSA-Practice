
public class Main
{
	public static void main(String[] args) {
		int[] rno=new int[5];
		String[] names=new String[5];
		float[] marks=new float[5];
		Student[] students=new Student[5];
		Student kunal= new Student();
		
		
		kunal.rno=13;
