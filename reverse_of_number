import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        // Reverse of number 
        // eg 12344 == ans =44321
        int num=in.nextInt();
        int ans=0;
        while (num>0){
            int rem=num %10;
            num/=10;
            
            ans=ans *10 + rem;
            
            
        }
       System.out.println(ans);
        
        }
}
